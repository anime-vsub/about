{
  "name": "vite-project",
  "private": true,
  "version": "0.0.4",
  "type": "module",
  "scripts": {
    "lint": "eslint --ext .ts,.vue ./",
    "pretit": "prettier \"**/*.{ts,vue,scss,html,md,json}\" --ignore-path .gitignore,.prettierignore",
    "format": "prettier --write \"**/*.{ts,vue,scss,html,md,json}\" --ignore-path .gitignore,.prettierignore",
    "build": "cross-env NODE_ENV=production vite-ssg build",
    "dev": "vite",
    "preview": "vite preview",
    "test": "echo 'No test'",
    "typing": "pnpm build && vue-tsc --noEmit",
    "ci": "pnpm format && pnpm lint --fix && pnpm typing",
    "release": "bumpp package.json --commit --tag --push"
  },
  "dependencies": {
    "@iconify/vue": "^4.0.2",
    "@vueuse/core": "^9.9.0",
    "@vueuse/head": "^1.0.22",
    "dayjs": "^1.11.7",
    "nprogress": "^0.2.0",
    "prism-theme-vars": "^0.2.4",
    "prismjs": "^1.29.0",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@tachibana-shin/eslint-config-vue": "^1.0.7",
    "@types/diacritics": "^1.3.1",
    "@types/fs-extra": "^9.0.13",
    "@types/markdown-it-link-attributes": "^3.0.1",
    "@types/node": "^18.11.18",
    "@types/nprogress": "^0.2.0",
    "@unocss/preset-icons": "^0.48.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vueuse/router": "^9.9.0",
    "bumpp": "^8.2.1",
    "cross-env": "^7.0.3",
    "diacritics": "^1.3.0",
    "eslint": "^8.30.0",
    "fs-extra": "^11.1.0",
    "gray-matter": "^4.0.3",
    "markdown-it-anchor": "^8.6.6",
    "markdown-it-link-attributes": "^4.0.1",
    "markdown-it-prism": "^2.3.0",
    "markdown-it-table-of-contents": "^0.6.0",
    "prettier": "^2.8.1",
    "sass": "^1.57.1",
    "typescript": "^4.9.3",
    "unocss": "^0.48.0",
    "unplugin-auto-import": "^0.12.1",
    "unplugin-icons": "^0.14.15",
    "unplugin-vue-components": "^0.22.12",
    "vite": "^4.0.0",
    "vite-plugin-inspect": "^0.7.11",
    "vite-plugin-md": "^0.21.5",
    "vite-plugin-pages": "^0.28.0",
    "vite-plugin-windicss": "^1.8.10",
    "vite-ssg": "^0.22.1",
    "vue-tsc": "^1.0.11",
    "windicss": "^3.5.6"
  }
}
